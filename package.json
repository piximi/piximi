    "start": "BROWSER='none' VITE_APP_VERSION=$npm_package_version vite",
    "start-logger": "BROWSER='none' VITE_APP_VERSION=$npm_package_version VITE_APP_LOG_LEVEL='1' vite",
    "test": "vite test",
